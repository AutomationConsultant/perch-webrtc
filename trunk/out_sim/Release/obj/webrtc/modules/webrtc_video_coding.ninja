    cxx ../../webrtc/modules/video_coding/main/source/qm_select.cc
build $
    obj/webrtc/modules/video_coding/main/source/webrtc_video_coding.receiver.o: $
    cxx ../../webrtc/modules/video_coding/main/source/receiver.cc
build $
