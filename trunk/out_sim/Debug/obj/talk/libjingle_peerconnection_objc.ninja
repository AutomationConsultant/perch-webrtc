defines = -DANGLE_DX11 -DEXPAT_RELATIVE_PATH -DFEATURE_ENABLE_VOICEMAIL $
    -DGTEST_RELATIVE_PATH -DJSONCPP_RELATIVE_PATH -DLOGGING=1 $
    -DSRTP_RELATIVE_PATH -DFEATURE_ENABLE_SSL -DFEATURE_ENABLE_PSTN $
    -DHAVE_SRTP -DHAVE_WEBRTC_VIDEO -DHAVE_WEBRTC_VOICE $
    -DUSE_WEBRTC_DEV_BRANCH -DDISABLE_NACL -DCHROMIUM_BUILD $
    -DUSE_LIBJPEG_TURBO=1 -DENABLE_INPUT_SPEECH -DENABLE_EGLIMAGE=1 $
    -DENABLE_SPELLCHECK=1 -DDISABLE_FTP_SUPPORT=1 -DLIBPEERCONNECTION_LIB=1 $
    -DIOS -DHAVE_NSS_SSL_H=1 -DSSL_USE_NSS_RNG -DCARBON_DEPRECATED=YES $
    -DHASH_NAMESPACE=__gnu_cxx -DPOSIX -DDISABLE_DYNAMIC_CAST -D_REENTRANT $
    -DDYNAMIC_ANNOTATIONS_ENABLED=1 -DWTF_USE_DYNAMIC_ANNOTATIONS=1
includes = -I../.. -I../../third_party -I../../third_party/webrtc $
    -I../../webrtc -I../../talk/app/webrtc -I../../talk/app/webrtc/objc $
    -I../../talk/app/webrtc/objc/public
cflags = -isysroot $
    /Applications/Xcode.app/Contents/Developer/Platforms/iPhoneSimulator.platform/Developer/SDKs/iPhoneSimulator6.1.sdk $
    -O0 -gdwarf-2 -fvisibility=hidden -Wnewline-eof $
    -mios-simulator-version-min=6.0 -arch i386 -Wendif-labels $
    -Wno-unused-parameter -Wno-missing-field-initializers -Wheader-hygiene $
    -Wno-c++11-narrowing -Wno-char-subscripts -Wno-unused-function $
    -Wno-covered-switch-default -Wstring-conversion $
    -Wno-deprecated-register -Wheader-hygiene -Wno-char-subscripts $
    -Wno-unused-function -Wno-unnamed-type-template-args -Wno-c++11-narrowing
cflags_c = -std=c99 -fcolor-diagnostics
cflags_cc = -std=gnu++0x -fno-rtti -fno-exceptions $
    -fvisibility-inlines-hidden -fno-threadsafe-statics -fcolor-diagnostics $
    -std=gnu++11
cflags_objc = $cflags_c -fobjc-arc
cflags_objcc = $cflags_cc -fobjc-arc -fobjc-call-cxx-cdtors

build $
    obj/talk/app/webrtc/objc/libjingle_peerconnection_objc.rtcaudiotrack.o: $
    objcxx ../../talk/app/webrtc/objc/RTCAudioTrack.mm
build $
    obj/talk/app/webrtc/objc/libjingle_peerconnection_objc.rtcenumconverter.o: $
    objcxx ../../talk/app/webrtc/objc/RTCEnumConverter.mm
build $
    obj/talk/app/webrtc/objc/libjingle_peerconnection_objc.rtci420frame.o: $
    objcxx ../../talk/app/webrtc/objc/RTCI420Frame.mm
build $
    obj/talk/app/webrtc/objc/libjingle_peerconnection_objc.rtcicecandidate.o: $
    objcxx ../../talk/app/webrtc/objc/RTCICECandidate.mm
build $
    obj/talk/app/webrtc/objc/libjingle_peerconnection_objc.rtciceserver.o: $
    objcxx ../../talk/app/webrtc/objc/RTCICEServer.mm
build $
    obj/talk/app/webrtc/objc/libjingle_peerconnection_objc.rtcmediaconstraints.o: $
    objcxx ../../talk/app/webrtc/objc/RTCMediaConstraints.mm
build $
    obj/talk/app/webrtc/objc/libjingle_peerconnection_objc.rtcmediaconstraintsnative.o: $
    cxx ../../talk/app/webrtc/objc/RTCMediaConstraintsNative.cc
build $
    obj/talk/app/webrtc/objc/libjingle_peerconnection_objc.rtcmediasource.o: $
    objcxx ../../talk/app/webrtc/objc/RTCMediaSource.mm
build $
    obj/talk/app/webrtc/objc/libjingle_peerconnection_objc.rtcmediastream.o: $
    objcxx ../../talk/app/webrtc/objc/RTCMediaStream.mm
build $
    obj/talk/app/webrtc/objc/libjingle_peerconnection_objc.rtcmediastreamtrack.o: $
    objcxx ../../talk/app/webrtc/objc/RTCMediaStreamTrack.mm
build obj/talk/app/webrtc/objc/libjingle_peerconnection_objc.rtcpair.o: $
    objc ../../talk/app/webrtc/objc/RTCPair.m
build $
    obj/talk/app/webrtc/objc/libjingle_peerconnection_objc.rtcpeerconnection.o: $
    objcxx ../../talk/app/webrtc/objc/RTCPeerConnection.mm
build $
    obj/talk/app/webrtc/objc/libjingle_peerconnection_objc.rtcpeerconnectionfactory.o: $
    objcxx ../../talk/app/webrtc/objc/RTCPeerConnectionFactory.mm
build $
    obj/talk/app/webrtc/objc/libjingle_peerconnection_objc.rtcpeerconnectionobserver.o: $
    objcxx ../../talk/app/webrtc/objc/RTCPeerConnectionObserver.mm
build $
    obj/talk/app/webrtc/objc/libjingle_peerconnection_objc.rtcsessiondescription.o: $
    objcxx ../../talk/app/webrtc/objc/RTCSessionDescription.mm
build $
    obj/talk/app/webrtc/objc/libjingle_peerconnection_objc.rtcvideocapturer.o: $
    objcxx ../../talk/app/webrtc/objc/RTCVideoCapturer.mm
build $
    obj/talk/app/webrtc/objc/libjingle_peerconnection_objc.rtcvideorenderer.o: $
    objcxx ../../talk/app/webrtc/objc/RTCVideoRenderer.mm
build $
    obj/talk/app/webrtc/objc/libjingle_peerconnection_objc.rtcvideosource.o: $
    objcxx ../../talk/app/webrtc/objc/RTCVideoSource.mm
build $
    obj/talk/app/webrtc/objc/libjingle_peerconnection_objc.rtcvideotrack.o: $
    objcxx ../../talk/app/webrtc/objc/RTCVideoTrack.mm

build libjingle_peerconnection_objc.a: alink $
    obj/talk/app/webrtc/objc/libjingle_peerconnection_objc.rtcaudiotrack.o $
    obj/talk/app/webrtc/objc/libjingle_peerconnection_objc.rtcenumconverter.o $
    obj/talk/app/webrtc/objc/libjingle_peerconnection_objc.rtci420frame.o $
    obj/talk/app/webrtc/objc/libjingle_peerconnection_objc.rtcicecandidate.o $
    obj/talk/app/webrtc/objc/libjingle_peerconnection_objc.rtciceserver.o $
    obj/talk/app/webrtc/objc/libjingle_peerconnection_objc.rtcmediaconstraints.o $
    obj/talk/app/webrtc/objc/libjingle_peerconnection_objc.rtcmediaconstraintsnative.o $
    obj/talk/app/webrtc/objc/libjingle_peerconnection_objc.rtcmediasource.o $
    obj/talk/app/webrtc/objc/libjingle_peerconnection_objc.rtcmediastream.o $
    obj/talk/app/webrtc/objc/libjingle_peerconnection_objc.rtcmediastreamtrack.o $
    obj/talk/app/webrtc/objc/libjingle_peerconnection_objc.rtcpair.o $
    obj/talk/app/webrtc/objc/libjingle_peerconnection_objc.rtcpeerconnection.o $
    obj/talk/app/webrtc/objc/libjingle_peerconnection_objc.rtcpeerconnectionfactory.o $
    obj/talk/app/webrtc/objc/libjingle_peerconnection_objc.rtcpeerconnectionobserver.o $
    obj/talk/app/webrtc/objc/libjingle_peerconnection_objc.rtcsessiondescription.o $
    obj/talk/app/webrtc/objc/libjingle_peerconnection_objc.rtcvideocapturer.o $
    obj/talk/app/webrtc/objc/libjingle_peerconnection_objc.rtcvideorenderer.o $
    obj/talk/app/webrtc/objc/libjingle_peerconnection_objc.rtcvideosource.o $
    obj/talk/app/webrtc/objc/libjingle_peerconnection_objc.rtcvideotrack.o
  libtool_flags = -framework AVFoundation -framework CoreMedia -framework $
      CoreVideo
